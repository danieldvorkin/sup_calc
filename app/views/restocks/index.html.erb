<div class="container restock-container row">
  <% @places.each do |place| %>
    <div class="col s12 m4 lg4">
      <%= link_to restock_country_path(place) do %>
        <div class="card" style="box-shadow: none !important;">
          <div class="card-content" style="padding: 0px !important;">
            <img style="box-shadow: 1px 1px 10px 1px silver;min-width:100% !important;" alt="<%= place.upcase %>" class="border--round" src="https://www.supremecommunity.com/s/img/restocks/<%= place %>.png">
            <span class="card-title" style="text-align:center !important;"><%= place.upcase %></span>
          </div>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
